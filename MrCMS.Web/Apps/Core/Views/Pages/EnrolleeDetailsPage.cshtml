                                            <table id="dependantlist" class="mdl-data-table mdl-data-table--selectable">
                                                <thead>
                                                    <tr>
                                                        <th>Id</th>
                                                        <th>Passport</th>

                                                        <th>Name</th>
                                                        <th>Policy Number</th>
                                                        <th>DOB</th>
                                                        <th>Sex</th>
                                                        <th>Hospital</th>
                                                        <th>Mobile Number</th>
                                                        <th>Pre-existing medical conditions</th>
