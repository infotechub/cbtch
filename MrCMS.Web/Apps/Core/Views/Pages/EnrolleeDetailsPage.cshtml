                                                    @Html.TextBox("email", Model.Enrolleemodel.Emailaddress, new { @class = "form-control", @placeholder = "Email", @id = "Email" })


                                                </td>


                                            </tr>

                                            <tr>
                                                <td>Sponsorship Type</td>
